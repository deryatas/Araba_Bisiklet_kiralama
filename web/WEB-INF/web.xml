<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Kontroller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Kontroller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Kontroller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateServlet</servlet-name>
        <servlet-class>Kontroller.UpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>Kontroller.AdminLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogoutServlet</servlet-name>
        <servlet-class>Kontroller.AdminLogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAracServlet</servlet-name>
        <servlet-class>araclist.AddAracServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAracServlet</servlet-name>
        <servlet-class>araclist.DeleteAracServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AracDeleteServlet</servlet-name>
        <servlet-class>araclist.AracDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteServlet</servlet-name>
        <servlet-class>Kontroller.DeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KiralaServlet</servlet-name>
        <servlet-class>Kontroller.KiralaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KiralaDeleteServlet</servlet-name>
        <servlet-class>Kontroller.KiralaDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateServlet</servlet-name>
        <url-pattern>/UpdateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/AdminLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogoutServlet</servlet-name>
        <url-pattern>/AdminLogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAracServlet</servlet-name>
        <url-pattern>/AddAracServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAracServlet</servlet-name>
        <url-pattern>/DeleteAracServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AracDeleteServlet</servlet-name>
        <url-pattern>/AracDeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteServlet</servlet-name>
        <url-pattern>/DeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KiralaServlet</servlet-name>
        <url-pattern>/KiralaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KiralaDeleteServlet</servlet-name>
        <url-pattern>/KiralaDeleteServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
